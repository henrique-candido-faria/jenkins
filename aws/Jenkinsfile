
node {
    stage("LIST CLUSTERS") {
        withCredentials([[$class: 'UsernamePasswordMultiBinding',
        credentialsId: 'aws-key',
        usernameVariable: 'AWS_ACCESS_KEY_ID',
        passwordVariable: 'AWS_SECRET_ACCESS_KEY']]) {
            script {
                LIST_CLUSTERS=
                aws emr list-clusters \
                    --region us-east-1 \
                    --active \
                    --output json \
                    --query 'Clusters[*].[Id]'
            }
        }
    }
}